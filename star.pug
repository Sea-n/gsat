doctype html
if gsatYear
	- var old = true
else
	- var gsatYear = 111
html
	head
		title 繁星推薦 - 學測五選四 #{gsatYear} 年度
		meta(name="keywords" content="學測五選四,繁星推薦,學測五選三")
		meta(property="og:url" content="https://sean.cat/gsat/star" + gsatYear)
		meta(property="og:title" content="學測五選四 - 繁星推薦")
		include includes/head.pug
		script(type="application/ld+json").
			{
				"@context": "http://schema.org",
				"@type": "BreadcrumbList",
				"itemListElement": [
					{
						"@type": "ListItem",
						"position": 1,
						"item": {
							"@type":"WebPage",
							"@id": "https://sean.cat/",
							"url":"https://sean.cat/",
							"name": "Sean's Website"
						}
					}, {
						"@type": "ListItem",
						"position": 2,
						"item": {
							"@type":"WebPage",
							"@id": "https://sean.cat/gsat/",
							"url":"https://sean.cat/gsat/",
							"name": "學測五選四"
						}
					}, {
						"@type": "ListItem",
						"position": 3,
						"item": {
							"@type":"WebPage",
							"@id": "https://sean.cat/gsat/star" + gsatYear,
							"url":"https://sean.cat/gsat/star" + gsatYear,
							"name": "繁星推薦 " + gsatYear + " 年度"
						}
					}
				]
			}
	body
		nav.ts.basic.fluid.borderless.menu.horizontally.scrollable
			.ts.container
				a.item(href=".") 首頁
				a.item.hide1(href="apply") 個人申請
				b.item.active.hide1 繁星推薦
				a.item.hide1(href="advanced") 指考分發
				.right.fitted.item
					a.item(target="_blank" href="https://www.sean.taipei/") 關於作者
		header.ts.fluid.vertically.padded.heading.slate
			.ts.narrow.container
				h1.header 繁星推薦
				.description 學測五選四 #{gsatYear} 學年度
		div.ts.container(name="main")
			include includes/search.pug
		include includes/footer.pug
		script.
			var gsatType = "star";
			var gsatYear = "#{gsatYear}";
		script(src="js/utils.js?v=0213")
		script(src="js/common.js?v=0213")
